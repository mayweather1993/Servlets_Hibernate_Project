<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>welcome</servlet-name>
    <jsp-file>/jsp/welcome.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>ManufacturerController</servlet-name>
    <servlet-class>net.mayweather.controller.Manufacturer.ManufacturerController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProductController</servlet-name>
    <servlet-class>net.mayweather.controller.Product.ProductControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>add-manufacturer-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Manufacturer.AddManufacturerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>list-manufacturer-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Manufacturer.ListAllManufacturerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>delete-manufacturer-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Manufacturer.DeleteManufacturerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>edit-manufacturer-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Manufacturer.EditManufacturerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>add-product-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Product.AddProductServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>list-product-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Product.ListAllProductServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>edit-product-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Product.EditProductServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>delete-product-servlet</servlet-name>
    <servlet-class>net.mayweather.controller.Product.DeleteProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManufacturerController</servlet-name>
    <url-pattern>/manufacturer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProductController</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>add-manufacturer-servlet</servlet-name>
    <url-pattern>/addM</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>list-manufacturer-servlet</servlet-name>
    <url-pattern>/listM</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>delete-manufacturer-servlet</servlet-name>
    <url-pattern>/deleteM</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>edit-manufacturer-servlet</servlet-name>
    <url-pattern>/editM</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>add-product-servlet</servlet-name>
    <url-pattern>/addP</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>list-product-servlet</servlet-name>
    <url-pattern>/listP</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>delete-product-servlet</servlet-name>
    <url-pattern>/deleteP</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>edit-product-servlet</servlet-name>
    <url-pattern>/editP</url-pattern>
  </servlet-mapping>
</web-app>
